   "source": [
    "def fizz_buzz(maximum):\n",
    "\tout=[]\n",
    "\tfor n in range(1,maximum+1):\n",
    "\t\tif n%5==0 and n%3==0:\tout.append(\"FizzBuzz\")\n",
    "\t\tif n%3==0:\tout.append(\"Fizz\")\n",
    "\t\tif n%5==0:\tout.append(\"Buzz\")\n",
    "\t\tif n%3!=0 and n%5!=0: out.append(n)\n",
    "\treturn out"
   ]
  },
